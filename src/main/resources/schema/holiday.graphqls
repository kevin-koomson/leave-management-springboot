    name: String
    description: String
    country: String
    startDay: String
    endDay: String
    archived: Boolean
}

input CreateHolidayInput {
    name: String!
    description: String
    country: String!
    startDay: String!
    endDay: String
}

input UpdateHolidayInput {
    id: ID!
